<?xml version="1.0" encoding="utf-8"?>
<MotionScene
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
        <KeyFrameSet>
        </KeyFrameSet>
        <OnSwipe motion:touchAnchorId="@+id/view" />
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView"
            motion:layout_constraintStart_toEndOf="@+id/textView3"
            motion:layout_constraintTop_toBottomOf="@+id/edtB"
            android:layout_marginTop="40dp"
            android:layout_marginLeft="84dp"
            android:layout_marginStart="84dp"
            motion:layout_constraintEnd_toStartOf="@+id/imageView2" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView2"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintTop_toBottomOf="@+id/edtB"
            motion:layout_constraintStart_toEndOf="@+id/imageView"
            android:layout_marginTop="40dp"
            motion:layout_constraintHorizontal_bias="0.375" />
        <Constraint
            motion:layout_constraintVertical_bias="0.0"
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintTop_toTopOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/view" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:layout_height="43dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.907"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="140dp"
            android:id="@+id/editTextTextPersonName4"
            motion:layout_constraintBottom_toBottomOf="@+id/view" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.336"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/textView4"
            motion:layout_constraintTop_toTopOf="@+id/editTextTextPersonName4"
            android:layout_marginTop="12dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/button"
            motion:layout_constraintBottom_toBottomOf="@+id/button3" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toEndOf="@+id/button"
            motion:layout_constraintHorizontal_bias="0.144"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:layout_marginStart="108dp"
            android:id="@+id/button2"
            motion:layout_constraintBottom_toBottomOf="@+id/button4" />
        <Constraint
            android:layout_height="209dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintBottom_toBottomOf="parent"
            android:layout_width="410dp"
            android:id="@+id/view" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintHorizontal_bias="0.325"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/button3"
            motion:layout_constraintBottom_toBottomOf="@+id/button4" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintHorizontal_bias="1.0"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toEndOf="@+id/button3"
            android:id="@+id/button4"
            motion:layout_constraintBottom_toTopOf="@+id/editTextTextPersonName4" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintHorizontal_bias="0.05"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/textView3"
            motion:layout_constraintBottom_toTopOf="@+id/button"
            android:layout_marginBottom="4dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintHorizontal_bias="0.107"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/textView2"
            motion:layout_constraintBottom_toTopOf="@+id/textView3"
            android:layout_marginBottom="12dp" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintStart_toEndOf="@+id/textView2"
            motion:layout_constraintHorizontal_bias="0.395"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            android:id="@+id/edtB"
            motion:layout_constraintBottom_toTopOf="@+id/textView3" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintHorizontal_bias="0.178"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toEndOf="@+id/textView"
            android:layout_marginStart="40dp"
            android:id="@+id/edtA"
            motion:layout_constraintBottom_toTopOf="@+id/edtB" />
        <Constraint
            android:layout_height="wrap_content"
            motion:layout_constraintHorizontal_bias="0.107"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="wrap_content"
            motion:layout_constraintStart_toStartOf="parent"
            android:id="@+id/textView"
            motion:layout_constraintBottom_toTopOf="@+id/textView2"
            android:layout_marginBottom="24dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView2"
            motion:layout_constraintBottom_toBottomOf="@+id/edtA"
            motion:layout_constraintEnd_toEndOf="@+id/view"
            android:layout_marginEnd="12dp"
            android:layout_marginRight="12dp" />
        <Constraint
            android:layout_height="wrap_content"
            android:layout_width="wrap_content"
            android:id="@+id/imageView"
            motion:layout_constraintBottom_toTopOf="@+id/button4"
            android:layout_marginBottom="36dp"
            motion:layout_constraintEnd_toEndOf="@+id/view"
            android:layout_marginEnd="12dp"
            android:layout_marginRight="12dp" />
    </ConstraintSet>
</MotionScene>